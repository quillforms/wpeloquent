{
    "name": "quillforms/wpeloquent",
    "description": "A custom package with modified Eloquent, Illuminate, and WP models",
    "type": "library",
    "autoload": {
        "psr-4": {
            "WPEloquent\\Eloquent\\": "src/Eloquent/",
            "Illuminate\\Container\\": "src/Illuminate/container/",
            "Illuminate\\Contracts\\": "src/Illuminate/contracts/",
            "Illuminate\\Database\\": "src/Illuminate/database/",
            "Illuminate\\Events\\": "src/Illuminate/events/",
            "Illuminate\\Filesystem\\": "src/Illuminate/filesystem/",
            "Illuminate\\Pagination\\": "src/Illuminate/pagination/",
            "Illuminate\\Support\\": "src/Illuminate/support/",
            "Illuminate\\Translation\\": "src/Illuminate/translation/",
            "Illuminate\\Validation\\": "src/Illuminate/validation/",
            "WPEloquent\\WP\\": "src/WP/"
        },
        "files": [
            "src/Illuminate/support/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "symfony/finder": "^4.2",
        "doctrine/inflector": "^1.1",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "symfony/http-foundation": "^4.2",
        "egulias/email-validator": "^2.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*"
    }
}