{
    "name": "quillforms/wpeloquent",
    "description": "A custom package with modified Eloquent, Illuminate, and WP models",
    "type": "library",
    "autoload": {
        "psr-4": {
            "WPEloquent\\Eloquent\\": "src/Eloquent/",
            "Illuminate\\": "src/Illuminate/",
            "WPEloquent\\WP\\": "src/WP/"
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "symfony/finder": "^4.2",
        "doctrine/inflector": "^1.1",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "symfony/http-foundation": "^4.2",
        "egulias/email-validator": "^2.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*"
    },
    "replace": {
        "illuminate/container": "*",
        "illuminate/contracts": "*",
        "illuminate/database": "*",
        "illuminate/events": "*",
        "illuminate/filesystem": "*",
        "illuminate/pagination": "*",
        "illuminate/support": "*",
        "illuminate/translation": "*",
        "illuminate/validation": "*"
    },
    "conflict": {
        "illuminate/container": "*",
        "illuminate/contracts": "*",
        "illuminate/database": "*",
        "illuminate/events": "*",
        "illuminate/filesystem": "*",
        "illuminate/pagination": "*",
        "illuminate/support": "*",
        "illuminate/translation": "*",
        "illuminate/validation": "*"
    }
}